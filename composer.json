{
    "name": "houfio/hydra",
    "type": "project",
    "require": {
        "php": "^7.4",
        "firebase/php-jwt": "^5.2",
        "laravel/lumen-framework": "^7.1"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
